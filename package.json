{
  "name": "msg-fabric-endpoint",
  "version": "0.5.13",
  "description": "Endpoints for msg-fabric",
  "license": "BSD-2-Clause",
  "author": "Shane Holloway <shane.holloway@ieee.org>",
  "homepage": "https://github.com/shanewholloway/msg-fabric-endpoint#readme",
  "bugs": {
    "url": "https://github.com/shanewholloway/msg-fabric-endpoint/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/shanewholloway/msg-fabric-endpoint.git"
  },
  "keywords": [
    "ecosystem:msg-fabric",
    "distributed live object",
    "message-passing",
    "distributed object",
    "distributed",
    "actors",
    "message",
    "passing"
  ],
  "files": [
    "cjs/",
    "esm/",
    "umd/"
  ],
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "browser": "umd/msg-fabric-endpoint.min.js",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-jsy": "^0.10.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "mocha": "^5.0.1",
    "msg-fabric-core": "^0.5.13",
    "rollup": "^0.56.3",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-jsy-babel": "^2.0.0",
    "rollup-plugin-node-resolve": "^3.0.3",
    "rollup-plugin-uglify": "^3.0.0",
    "uglify-es": "^3.3.9"
  },
  "babel": {
    "presets": "jsy/lean"
  },
  "scripts": {
    "clean": "rm cjs/* esm/* umd/*",
    "build": "rollup --config",
    "watch": "npm -s run build -- --watch",
    "prepublish": "npm -s test",
    "pretest": "npm -s run build",
    "test": "npm -s run test:one",
    "test:one": "mocha --require babel-core/register test/unittest/node.js",
    "test:watch": "mocha --watch --require babel-core/register test/unittest/node.js",
    "test:named": "mocha -r babel-core/register -r test/unittest/node0.js",
    "demo": "babel-node test/manual/pizza_factory.jsy"
  }
}
