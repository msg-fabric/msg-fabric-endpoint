export const ep_proto = Object.create @
  Object.getPrototypeOf @ function(){}
  @{} _unwrap_: @{} get: _unwrap_

function _unwrap_() ::
  const self = Object.create(this)
  self.endpoint = v => v
  return self

export function add_ep_kind(kinds) ::
  Object.assign @ ep_proto, kinds
export default add_ep_kind

